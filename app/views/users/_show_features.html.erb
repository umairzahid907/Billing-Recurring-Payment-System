<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Units used</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% features.each do |feature| %>
      <tr>
        <td><%= feature.name %></td>
        <td><%= feature.unit_price %></td>
        <td><%= feature.max_unit_limit %></td>
        <td><%= feature.usages.find_by(subscription_id: ) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
